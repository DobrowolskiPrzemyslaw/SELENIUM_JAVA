<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- dwa testy jeden po drugim z przekazaniem parametru z pliku .xml-->
<suite name="Zestaw testÃ³w Shopify E2E">
    <parameter name="browserName" value="chrome"></parameter>                       <!--parametry globalne dla wielu testu - W TAGU suit, przekazenie parametry do @BeforeTest-->
    <parameter name="env" value="https://simpletheme.myshopify.com/"></parameter>   <!--parametry globalne dla wielu testu - W TAGU suit, przekazenie parametry do @BeforeTest-->
    <test name="Test E2E 1">
        <classes>
            <class name="seleniumB2B.shopifyTests.ShopifyTest">
                <parameter name="Item2" value="Boot"></parameter>                   <!--przekazenie parametry do @Test - W TAGU class-->
                <methods>
                    <include name="shopifyFirstTest2"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Test E2E 2">
        <classes>
            <class name="seleniumB2B.shopifyTests.ShopifyTest">
                <parameter name="Item" value="Goran Zip Boot - Rust"></parameter>   <!--przekazenie parametry do @Test - W TAGU class-->
                <methods>
                    <include name="shopifyFirstTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>